local function verifyWhitelist()
    local localUserIds = {
        694435774,
        5586940247,
        7319184415,
        2982476041,
        1008601724,
        8233218780,
        7381719922
    }
    
    local player = game.Players.LocalPlayer
    local localCheck = false
    
    for _, id in ipairs(localUserIds) do
        if player.UserId == id then
            localCheck = true
            break
        end
    end
    
    if not localCheck then
        return false
    end
    
    local remoteSuccess, remoteResult = pcall(function()
        local whitelistUrl = "https://raw.githubusercontent.com/CrimsonXPo3/CrimsonHub/refs/heads/main/Whitelist"
        local whitelistScript = game:HttpGet(whitelistUrl, true)
        local whitelistFunc = loadstring(whitelistScript)
        return whitelistFunc()
    end)
    
    if not remoteSuccess then
        return false
    end
    
    if remoteResult ~= true then
        return false
    end
    
    return true
end

if not verifyWhitelist() then
    return
end
