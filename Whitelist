local success, isWhitelisted = pcall(function()
    local allowedUserIds = {
        694435774,
        5586940247,
        7319184415,
        2982476041,
        1008601724,
        8233218780,
        7381719922
    }
    
    local player = game.Players.LocalPlayer

    if not player or not player.UserId then
        error("Failed to get player information")
    end

    for _, id in ipairs(allowedUserIds) do
        if player.UserId == id then
            print("[Whitelist] User is whitelisted!")
            return true
        end
    end
    
    return false
end)

if not success then
    return
end

if not isWhitelisted then
    return
end
